{
  "name": "mdit-plugin-callouts",
  "type": "module",
  "version": "1.0.3",
  "description": "",
  "packageManager": "pnpm@7.12.2",
  "author": "widcardw",
  "license": "MIT",
  "homepage": "https://github.com/widcardw/mdit-plugin-callouts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/widcardw/mdit-plugin-callouts.git"
  },
  "bugs": "https://github.com/widcardw/mdit-plugin-callouts/issues",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "style": ["dist/index.css", "dist/vp.css"],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts --format cjs,esm --dts && postcss ./src/styles/default.css > ./dist/index.css && postcss ./src/styles/vp.css > ./dist/vp.css",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "markdown-it",
    "custom container"
  ],
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@types/markdown-it": "^12.2.3",
    "@types/rollup-plugin-css-only": "^3.1.0",
    "bumpp": "^8.2.1",
    "cssnano": "^5.1.14",
    "eslint": "^8.24.0",
    "markdown-it": "^13.0.1",
    "pnpm": "^7.12.2",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "tsup": "^6.4.0",
    "typescript": "^4.8.4",
    "vitest": "^0.23.4"
  }
}
